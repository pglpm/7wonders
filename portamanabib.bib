@Book{truesdell1969_r1984,
author =       {Truesdell, III, Clifford Ambrose},
title =	       {Rational Thermodynamics},
publisher =    {Springer},
year =	       {1984},
address =      {New York},
edition =      {2},
note =	       {First publ.\ 1969},
mynote =       {\begin{quotation}Looking back at the table of primitives and axioms, some will require that the definitions in a physical theory be ``operational'' and that the axioms be ``proved'' by experiments. It is a waste of time to argue with these people, because their definitions of ``operational'' and ``proved'' are circular and hence their contentions are irrefutable (as well as insupportable). It is easiest simply to admit that they are right and leave the matter of constructing the operational definitions and the experimental proofs to them. Clearly such metaphysical paraphernalia are not a part of the mathematical theory, and it is only the theory that I have attempted to present. Just as clearly, such paraphernalia are equally relevant or equally irrelevant to all theories. If they are needed in thermodynamics, they are needed also in mechanics. In mechanics, questions about what force and mass really are and how they are to be measured are important, but they are rarely mentioned in books on the mathematical theory. Persons who wish to study them are referred to works on metaphysics, the philosophical-empirical foundations of physics, or experimental procedures, according to taste. With thermodynamics it is a different matter. Whenever anyone tries to do anything rational there, no matter what branch or application he takes up, he may be sure to be pestered by professionals who rise and demand he explain how to define temperature and entropy for systems not infinitesimally near to equilibrium. The persecutor himself, obviously, regards ``equilibrium'' and ``infinitesimally near'' as primitive concepts. [Lecture~1, pp.~78--79]\end{quotation}},
origdate =     {1969}
}

